{
  "extends": "airbnb",
  "rules": {
    "no-irregular-whitespace": "error",
    "no-trailing-spaces": "error",
    "no-unused-vars": "error",
    "semi": "off",
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": "off"
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true
  },
  "env": {
    "jest": true
  },
  "parser": "babel-eslint"
}